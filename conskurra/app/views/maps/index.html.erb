<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <style>
      html, body, #map-canvas {
        height: 100%;
        margin: 0px;
        padding: 0px
      }
    </style>
  </head>
  <body>
    <button id="my_button" onclick="loadLocations()">Load Locations</button>
    <div id="map-canvas"></div>
    <div id="my-div">
	    <%if current_user %>
	      <%unless @maps.empty? %>
          <% @maps.each do |item| %>
		        <div class="map" data-marker='{"l": <%= item["l"]%> ,"n":<%= item["n"]%> }' > </div>
		      <%end%> 
		    <%end%>
      <%end %>
    </div>
  </body>
</html>
